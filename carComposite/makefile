	g++ -c -g Hub.cpp

main.o:
	g++ -c -g main.cpp

MediumCompound.o:
	g++ -c -g MediumCompound.cpp

OKCondition.o:
	g++ -c -g OKCondition.cpp

RaceCar.o:
	g++ -c -g RaceCar.cpp

RaceCarBuilder.o:
	g++ -c -g RaceCarBuilder.cpp

SoftCompound.o:
	g++ -c -g SoftCompound.cpp

Suspension.o:
	g++ -c -g Suspension.cpp

Team.o:
	g++ -c -g Team.cpp

